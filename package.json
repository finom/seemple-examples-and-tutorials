{
  "name": "seemple-examples",
  "version": "1.0.0",
  "description": "Tiny Seemple.js examples",
  "scripts": {
    "test": "eslint . --ignore-pattern 'node_modules'  --ignore-pattern 'vendor'",
    "dev": "http-server -p 8034 -o -s",
    "check-updates": "ncu",
    "upgrade": "npm run check-updates -- --upgradeAll && npm install",
    "compile-todomvc-docs": "docco --output ./todomvc/docs ./todomvc/js/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seemplejs/examples.git"
  },
  "keywords": [
    "seemple",
    "seemplejs"
  ],
  "author": "Andrey Gubanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seemplejs/examples/issues"
  },
  "homepage": "https://github.com/seemplejs/examples#readme",
  "devDependencies": {
    "docco": "^0.8.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "http-server": "^0.12.1",
    "npm-check-updates": "^4.0.1"
  }
}
