{
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "indent": ["error", 4],
        "no-var": "error",
        "comma-dangle": ["error", "never"],
        "class-methods-use-this": 0,
        "no-unused-vars": ["error", { "varsIgnorePattern": "Tree|Track|Phone" }],
        "no-restricted-syntax": 0
    },
    "env": {
        "browser": true
    },
    "globals": {
        "Seemple": true
    }
}
